<div class="TM-body">

    <div class="row">
            <div id="myCarousel" class="carousel slide" data-ride="carousel">
                <!-- Indicators -->
                <ol class="carousel-indicators">
                    <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                    <li data-target="#myCarousel" data-slide-to="1"></li>
                    <li data-target="#myCarousel" data-slide-to="2"></li>
                </ol>

                <!-- Wrapper for slides -->
                <div class="carousel-inner">
                  <div class="carousel-caption">
                      <h2>Design custom T-shirts & More</h2>
                      <h4>Your Design. Quality Apparel. Free Shipping.</h4>
                      <a style="" href="/products" type="button" class="btn btn-success custom-button">Get Started</a>
                  </div>
                    <div class="item active">
                        <img src="/img/banner/banner11.png" alt="Los Angeles" style="width:100%;">
                    </div>

                    <div class="item">
                        <img src="/img/banner/banner5.png" alt="Chicago" style="width:100%;">

                    </div>

                    <div class="item">
                        <img src="/img/banner/banner8.png" alt="New York" style="width:100%;">

                    </div>

                </div>

                <!-- Left and right controls -->
                <a class="left carousel-control" href="#myCarousel" data-slide="prev">
                    <span class="glyphicon glyphicon-chevron-left"></span>
                    <span class="sr-only">Previous</span>
                </a>
                <a class="right carousel-control" href="#myCarousel" data-slide="next">
                    <span class="glyphicon glyphicon-chevron-right"></span>
                    <span class="sr-only">Next</span>
                </a>
            </div>
        </div>
    </div>

    <div class="row" style="padding-top: 20px;">
        <div class="col-sm-8">
            <img src="/img/banner/banner9.png" style="width: 100%">
        </div>
        <div class="col-sm-4">
            <img src="/img/banner/banner3.png" style="width: 100%">
        </div>
        <!--<div class="col-sm-3">-->
            <!--<img src="/img/banner/banner3.png" style="width: 100%">-->
        <!--</div>-->
    </div>


    <div class="row TM-categories" style="background: white; margin: 20px">
        <h3 style="padding-left: 20px;">Most Popular</h3>
        <div class="col-sm-3 categories">
            <ul>
                <li>
                    <label class="container li-container">Trending
                        <input type="checkbox" checked="checked">
                        <span class="checkmark"></span>
                    </label>
                </li>
                <li>
                    <label class="container li-container">Sports
                        <input type="checkbox">
                        <span class="checkmark"></span>
                    </label>
                </li>
                <li>
                    <label class="container li-container">Parties & Events
                        <input type="checkbox">
                        <span class="checkmark"></span>
                    </label>
                </li>
                <li>
                    <label class="container li-container">Top Best-sellers
                        <input type="checkbox">
                        <span class="checkmark"></span>
                    </label>
                </li>
                <li>
                    <label class="container li-container">Business
                        <input type="checkbox">
                        <span class="checkmark"></span>
                    </label>
                </li>
                <li>
                    <label class="container li-container">College
                        <input type="checkbox">
                        <span class="checkmark"></span>
                    </label>
                </li>
                <li>
                    <label class="container li-container">Charities & Fundraisers
                        <input type="checkbox">
                        <span class="checkmark"></span>
                    </label>
                </li>
                <li>
                    <label class="container li-container">Religious
                        <input type="checkbox">
                        <span class="checkmark"></span>
                    </label>
                </li>
                <li>
                    <label class="container li-container">Holidays & Seasons
                        <input type="checkbox">
                        <span class="checkmark"></span>
                    </label>
                </li>
                <li>
                    <label class="container li-container">Test
                        <input type="checkbox">
                        <span class="checkmark"></span>
                    </label>
                </li>
                <li>
                    <label class="container li-container">Test
                        <input type="checkbox">
                        <span class="checkmark"></span>
                    </label>
                </li>
            </ul>
        </div>
        <div class="col-sm-9 book-list">
            <div class="container-fluid">
                {{#each products}}
                    {{#if isBreakLine}}
                    <div class="row" style="margin-bottom: 30px;">
                    {{/if}}
                    <div class="col-sm-3">
                        <a href="/products/{{id}}">
                            <img src="{{imagePath1}}" width="100%">
                            <div class="percent">{{discount}}%</div>
                            <p class="book-title">{{name}}</p>
                        </a>
                        <p><span class="price">{{formatNumber discountPrice style="currency" currency="USD"}}</span> <span class="original-price">{{formatNumber price style="currency" currency="USD"}}</span></p>
						<div class="size">Size:
							<span class="size">{{minSizeLatin}}</span>
							-
							<span class="size">{{maxSizeLatin}}</span>
						</div>
                    </div>
                    {{#if isBreakLine}}
                    </div>
                    {{/if}}
                {{/each}}
            </div>

            <div class="center">
                {{{paginateHelper pagination leftText="Prev" rightText="Next" paginationClass="pagination"}}}
            </div>

            <!-- <div class="center">
                <div class="pagination">
                    <a href="#">&laquo;</a>
                    <a href="#">1</a>
                    <a class="active" href="#">2</a>
                    <a href="#">3</a>
                    <a href="#">4</a>
                    <a href="#">5</a>
                    <a href="#">6</a>
                    <a href="#">&raquo;</a>
                </div>
            </div> -->
        </div>
    </div>
</div>
